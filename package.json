{
  "name": "el-tiptap",
  "version": "0.0.0-development",
  "license": "MIT",
  "private": true,
  "description": "A WYSIWYG editor based on Tiptap and Element.",
  "scripts": {
    "serve": "vue-cli-service serve --open examples/main.js",
    "build:lib": "rollup -c --environment BUILD:production",
    "build:example": "vue-cli-service build --dest dist examples/main.js",
    "lint": "vue-cli-service lint",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Leecason/el-tiptap.git"
  },
  "keywords": [
    "editor",
    "element",
    "element-ui",
    "tiptap",
    "prosemirror",
    "vue.js",
    "wysiwyg"
  ],
  "author": "leecason",
  "bugs": {
    "url": "https://github.com/Leecason/el-tiptap/issues"
  },
  "homepage": "https://github.com/Leecason/el-tiptapr#readme",
  "dependencies": {
    "core-js": "^3.4.3",
    "element-ui": "^2.13.0",
    "tiptap": "^1.26.5",
    "tiptap-extensions": "^1.28.5",
    "vue-awesome": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-component": "^1.1.1",
    "conventional-changelog-cli": "^2.0.28",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^9.4.3",
    "node-sass": "^4.12.0",
    "postcss-cssnext": "^3.1.0",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "^5.1.4",
    "sass-loader": "^8.0.0",
    "semantic-release": "^15.13.31",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.10"
  },
  "peerDependencies": {
    "vue": "^2.6.0",
    "element-ui": "^2.13.0",
    "vue-awesome": "^4.0.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
